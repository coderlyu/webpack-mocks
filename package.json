{
  "name": "@vdian/v-mock",
  "version": "1.0.0",
  "description": "mock data for vbuilder",
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "tsc": "tsc --module commonjs",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.ts",
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts",
    "watch": "cross-env NODE_ENV=development rollup -cw rollup.config.ts",
    "dev": "nodemon --watch src -e ts,tsx --exec ts-node src/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/http-proxy": "^1.17.8",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.6",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-cors": "^2.0.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3",
    "lodash.camelcase": "^4.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.3",
    "devcert": "^1.2.0",
    "fs-extra": "^10.0.1",
    "http-proxy": "^1.18.1",
    "json-schema-faker": "^0.5.0-rcv.41",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.4.1",
    "minimist": "^1.2.6",
    "portfinder": "^1.0.28",
    "rollup-plugin-terser": "^7.0.2",
    "typescript-json-schema": "^0.53.0"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
